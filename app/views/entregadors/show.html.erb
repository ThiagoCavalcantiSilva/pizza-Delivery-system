<p id="notice"><%= notice %></p>

<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 style="color: #fd7e14" class="card-title"><%= @entregador.nome %></h5>
    <h6 style="color: #000000" class="card-subtitle mb-2"><%= @entregador.telefone %></h6>
    <h6 style="color: #000000" class="card-subtitle mb-2"><%= @entregador.cpf %></h6>
    <%= link_to 'Editar', edit_entregador_path(@entregador), class:"btn btn-outline-dark" %> 
    <%= link_to 'Voltar', entregadors_path, class:"btn btn-outline-dark" %>  
  </div>
</div>

